							"pm.test(\"Status code is [200]\", function () {\r",
							"    pm.expect(pm.response.code).to.be.oneOf([200]);\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"Response contains 'version'\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData).to.have.property('version');\r",
							"});\r",
							"\r",
							"pm.test(\"Response matches ['nom']\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData['nom']).to.eql(\"SVIConsultation\")\r",
							"});\r",
							"\r",
							"pm.test(\"Response matches ['codeSAP']\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData['codeSAP']).to.eql(\"PXI26\")\r",
